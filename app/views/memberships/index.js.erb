var group = <%= current_group.to_param } %>,
    dict = <%= @all_members.to_json.html_safe %>,
    items = [];
  
$.each(dict, function(key, val){
  items.push(key);
});

$(function() {
  $('#auto_memb').typeahead({
    source: items,
    updater:function (item){
      window.location.href = '/groups/' + group + '/memberships/' + dict[item]; 
    }
  });
});
