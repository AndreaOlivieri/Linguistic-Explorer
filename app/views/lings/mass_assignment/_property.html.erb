<div class="fluid-container">
  <div id="prop-title" class="fluid-row">
    <h2>Property Description</h2>
  </div>

  <div id="prop-name" class="fluid-row">
    <h4> Property Name </h4>
    <p>
      <%= property.name %>
    </p>
  </div>
  
  <div id="prop-descrip" class="fluid-row">
    <h4> Property Description </h4>
    <p>
      <% if property.description %>
        <%= property.description %>
      <% else %>
        no property description available
      <% end %>
    </p>
  </div>
</div>


