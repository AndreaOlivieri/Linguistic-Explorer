.row
  = render :partial => 'searches/results_navbar'
#search_results.padded_left.padded_right{:data => {:query => @query }}
  .apple_pagination.js-pagination
  #pagination_table
    .progress.progress-striped{ :style => 'margin: 10px 20px 5px 30px;' }
      #results_loading_bar.progress-bar{ :aria => {:valuemin => "0", :valuemax=>"100"}, :style => "width: 0%;"}
    %p#results_loading_text.strong{ :style => 'text-align: center;'}
      Your query is processing...
    #error_message.alert.alert-danger.hide
      %div
        | Start a new search here:
        %span
          = link_to 'New Search', [:new, @group, :search], class: "btn"
  .apple_pagination.js-pagination